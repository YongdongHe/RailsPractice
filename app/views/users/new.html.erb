<% provide(:title, 'Sign up') %>
<div class="row">
    <div class="title-content">
        <h1>Sign up</h1>
    </div>
</div>

<div class="row">
  <div class="container col-md-6 main-content">
    <%= form_for(@user) do |field| %> 
        <%= render 'fields',:f =>field %>
        <br>
        <%= field.submit "Create my account", class: "form-control btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>

<h1>New user</h1>

<%= render 'form' %>

<%= link_to 'Back', users_path %>

